<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="BetTrack.Views.DashboardPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="{Binding Title}"
    BackgroundColor="#3B3B3B">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="{Binding Title}"
            TextColor="#F9B949"
            VerticalOptions="Center" />
        <Button Command="{Binding ShowRegistroCommand}" Text="Dashboard" />

    </VerticalStackLayout>
</ContentPage>